      <Button fx:id="nameButton" layoutX="231.0" layoutY="324.0" mnemonicParsing="false" onAction="#changeRoomName" text="Change Name" />
      <Button fx:id="capacityButton" layoutX="231.0" layoutY="374.0" mnemonicParsing="false" onAction="#incrementRoomCapacity" text="Increment Capacity" />
      <Button fx:id="descriptionButton" layoutX="379.0" layoutY="448.0" mnemonicParsing="false" onAction="#changeRoomDescription" text="Enter Description" />
      <TextField fx:id="priceField" layoutX="54.0" layoutY="546.0" />
      <Button fx:id="priceButton" layoutX="231.0" layoutY="546.0" mnemonicParsing="false" onAction="#changeRoomPrice" text="Change Price" />
      <Button layoutX="323.0" layoutY="65.0" mnemonicParsing="false" onAction="#load" text="Load" />
   </children>
</AnchorPane>
