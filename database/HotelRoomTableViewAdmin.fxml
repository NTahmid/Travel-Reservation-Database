<AnchorPane id="AnchorPane" prefHeight="599.0" prefWidth="718.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.HotelRoomTableViewAdminController">
   <children>
      <Button fx:id="backButton" layoutX="33.0" layoutY="31.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <TableView fx:id="table" layoutX="30.0" layoutY="100.0" prefHeight="200.0" prefWidth="680.0">
        <columns>
          <TableColumn fx:id="c1" prefWidth="75.0" text="Room id" />
          <TableColumn fx:id="c2" prefWidth="81.0" text="Room No" />
            <TableColumn fx:id="c3" prefWidth="130.0" text="Name" />
            <TableColumn fx:id="c4" prefWidth="76.0" text="Capacity" />
            <TableColumn fx:id="c5" minWidth="9.0" prefWidth="236.0" text="Description" />
            <TableColumn fx:id="c6" prefWidth="81.0" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="deleteButton" layoutX="600.0" layoutY="324.0" mnemonicParsing="false" onAction="#deleteRoom" text="Delete" />
      <TextField fx:id="nameField" layoutX="54.0" layoutY="324.0" />
      <TextField fx:id="capacityField" layoutX="54.0" layoutY="374.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
