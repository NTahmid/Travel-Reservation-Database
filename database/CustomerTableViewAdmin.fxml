<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="560.0" prefWidth="665.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.CustomerTableViewAdminController">
   <children>
      <TableView fx:id="table" layoutX="21.0" layoutY="149.0" prefHeight="200.0" prefWidth="633.0">
        <columns>
          <TableColumn fx:id="c1" prefWidth="107.0" text="Customer Name" />
          <TableColumn fx:id="c2" prefWidth="100.0" text="Username" />
            <TableColumn fx:id="c3" prefWidth="101.0" text="Address" />
            <TableColumn fx:id="c4" prefWidth="120.0" text="Contact" />
            <TableColumn fx:id="c5" prefWidth="93.0" text="Country" />
            <TableColumn fx:id="c6" prefWidth="109.0" text="City" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="31.0" layoutY="25.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <ComboBox fx:id="cityBox" layoutX="490.0" layoutY="77.0" prefWidth="150.0" promptText="Select City" />
      <ComboBox fx:id="countryBox" layoutX="52.0" layoutY="77.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <Button fx:id="searchButton" layoutX="307.0" layoutY="387.0" mnemonicParsing="false" onAction="#searchHotel" text="Search" />
   </children>
</AnchorPane>
