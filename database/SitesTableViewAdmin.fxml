<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="436.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.SitesTableViewAdminController">
   <children>
      <TableView fx:id="table" layoutX="87.0" layoutY="123.0" prefHeight="226.0" prefWidth="427.0">
        <columns>
          <TableColumn fx:id="c1" prefWidth="168.0" text="Site Name" />
          <TableColumn fx:id="c2" prefWidth="114.0" text="City" />
            <TableColumn fx:id="c3" prefWidth="144.0" text="Country" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="50.0" layoutY="33.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <ComboBox fx:id="countryBox" layoutX="210.0" layoutY="33.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <ComboBox fx:id="cityBox" layoutX="417.0" layoutY="33.0" onAction="#comboEvent1" prefWidth="150.0" promptText="Select City" />
      <Button fx:id="deleteButton" layoutX="275.0" layoutY="377.0" mnemonicParsing="false" onAction="#deleteSites" text="Delete" />
   </children>
</AnchorPane>
