<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="460.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="projectver2.CityTableViewAdminController">
   <children>
      <TableView fx:id="table" layoutX="85.0" layoutY="120.0" prefHeight="200.0" prefWidth="431.0">
        <columns>
          <TableColumn fx:id="c1" prefWidth="103.0" text="City Name" />
          <TableColumn fx:id="c2" prefWidth="162.0" text="Country Name" />
            <TableColumn fx:id="c3" prefWidth="165.0" text="City Id" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="36.0" layoutY="24.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <ComboBox fx:id="countryBox" layoutX="402.0" layoutY="24.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <Button fx:id="deleteButton" layoutX="235.0" layoutY="347.0" mnemonicParsing="false" onAction="#deleteCity" prefHeight="25.0" prefWidth="131.0" text="Delete Selected City" />
   </children>
</AnchorPane>
