<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="571.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.TourPackageFXMLController">
   <children>
      <Button fx:id="backButton" layoutX="31.0" layoutY="22.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <Text layoutX="82.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Package Name:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="packageNameField" layoutX="380.0" layoutY="77.0" />
      <Text layoutX="82.0" layoutY="151.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Package Id:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="packageIdField" layoutX="380.0" layoutY="133.0" />
      <Text layoutX="82.0" layoutY="213.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Price:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="priceField" layoutX="380.0" layoutY="195.0" />
      <Text layoutX="82.0" layoutY="268.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Number of Packages:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="packageNumberField" layoutX="380.0" layoutY="250.0" />
      <Button fx:id="enterButton" layoutX="278.0" layoutY="523.0" mnemonicParsing="false" onAction="#enterData" text="Enter" />
      <ComboBox fx:id="countryBox" layoutX="380.0" layoutY="346.0" onAction="#comboEvent1" prefWidth="150.0" promptText="Select Country" />
      <ComboBox fx:id="cityBox" layoutX="380.0" layoutY="395.0" onAction="#comboEvent2" prefWidth="150.0" promptText="Select City" />
      <ComboBox fx:id="companyBox" layoutX="380.0" layoutY="443.0" prefWidth="150.0" promptText="Select Company" />
      <Text fx:id="warningText" fill="#f80505" layoutX="230.0" layoutY="499.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please fill up all the entries">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
      <TextField fx:id="daysField" layoutX="381.0" layoutY="298.0" />
      <Text layoutX="82.0" layoutY="315.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Number of Days:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
