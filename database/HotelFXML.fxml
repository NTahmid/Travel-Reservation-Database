<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="547.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.HotelFXMLController">
   <children>
      <Button fx:id="backButton" layoutX="33.0" layoutY="29.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <Text layoutX="118.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Hotel Id:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="hotelIdField" layoutX="345.0" layoutY="87.0" />
      <Text layoutX="118.0" layoutY="157.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Hotel Name:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="hotelNameField" layoutX="345.0" layoutY="139.0" />
      <Text layoutX="118.0" layoutY="215.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Address:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="addressField" layoutX="345.0" layoutY="197.0" />
      <Text layoutX="118.0" layoutY="273.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Contact No:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="contactField" layoutX="345.0" layoutY="255.0" />
      <Text layoutX="118.0" layoutY="328.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Hotel Rating:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="ratingField" layoutX="345.0" layoutY="310.0" />
      <ComboBox fx:id="countryBox" layoutX="345.0" layoutY="374.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <ComboBox fx:id="cityBox" layoutX="345.0" layoutY="429.0" prefWidth="150.0" promptText="Select City" />
      <Button fx:id="enterButton" layoutX="278.0" layoutY="498.0" mnemonicParsing="false" onAction="#enterData" text="Enter" />
      <Text fx:id="warningText" fill="#f80303" layoutX="230.0" layoutY="484.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please fill up all the entries">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
