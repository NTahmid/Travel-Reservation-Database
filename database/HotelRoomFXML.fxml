<AnchorPane id="AnchorPane" prefHeight="660.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.HotelRoomFXMLController">
   <children>
      <Button fx:id="backButton" layoutX="26.0" layoutY="14.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
