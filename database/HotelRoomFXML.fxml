      <ComboBox fx:id="cityBox" layoutX="363.0" layoutY="502.0" onAction="#comboEvent2" prefWidth="150.0" promptText="Select City" />
      <ComboBox fx:id="hotelBox" layoutX="363.0" layoutY="551.0" prefWidth="150.0" promptText="Select Hotel" />
      <Text fx:id="warningText" fill="#f50707" layoutX="230.0" layoutY="609.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please fill up all the entries">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
      <Text layoutX="98.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Room Name:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="roomNameField" layoutX="356.0" layoutY="46.0" />
