      <Text layoutX="98.0" layoutY="113.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Room No:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="roomNoField" layoutX="356.0" layoutY="95.0" />
      <TextField fx:id="roomCapacityField" layoutX="356.0" layoutY="159.0" />
      <Text layoutX="98.0" layoutY="177.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Room Capacirty:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <Text layoutX="98.0" layoutY="238.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Price of Room:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="priceField" layoutX="356.0" layoutY="220.0" />
      <Text layoutX="98.0" layoutY="295.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Room Description:">
         <font>
