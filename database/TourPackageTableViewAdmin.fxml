<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="663.0" prefWidth="690.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.TourPackageTableViewAdminController">
   <children>
      <Button fx:id="backButton" layoutX="40.0" layoutY="35.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <TableView fx:id="table" layoutX="27.0" layoutY="166.0" prefHeight="262.0" prefWidth="636.0">
        <columns>
          <TableColumn fx:id="c1" prefWidth="110.0" text="Package Name" />
          <TableColumn fx:id="c2" prefWidth="73.0" text="Price" />
            <TableColumn fx:id="c3" prefWidth="88.0" text="No. Available" />
            <TableColumn fx:id="c4" prefWidth="92.0" text="Length of Days" />
            <TableColumn fx:id="c5" prefWidth="95.0" text="Company" />
            <TableColumn fx:id="c6" prefWidth="75.0" text="City" />
            <TableColumn fx:id="c7" prefWidth="102.0" text="Country" />
        </columns>
      </TableView>
      <Button fx:id="deleteButton" layoutX="577.0" layoutY="456.0" mnemonicParsing="false" onAction="#deletePackage" text="Delete" />
      <TextField fx:id="nameField" layoutX="40.0" layoutY="456.0" />
      <TextField fx:id="numberField" layoutX="40.0" layoutY="505.0" />
      <TextField fx:id="lengthField" layoutX="40.0" layoutY="554.0" />
      <TextField fx:id="priceField" layoutX="40.0" layoutY="603.0" />
      <Button fx:id="nameButton" layoutX="209.0" layoutY="456.0" mnemonicParsing="false" onAction="#changePackageName" text="Change Name" />
      <Button fx:id="amountButton" layoutX="210.0" layoutY="505.0" mnemonicParsing="false" onAction="#incrementPackageNumber" prefHeight="25.0" prefWidth="166.0" text="Increment amount avaiable" />
      <Button fx:id="lengthButton" layoutX="212.0" layoutY="554.0" mnemonicParsing="false" onAction="#incrementPackageDays" text="Increment Length of Days" />
      <Button fx:id="priceButton" layoutX="209.0" layoutY="603.0" mnemonicParsing="false" onAction="#changePackagePrice" prefHeight="25.0" prefWidth="92.0" text="Change Price" />
      <ComboBox fx:id="countryBox" layoutX="160.0" layoutY="35.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <ComboBox fx:id="cityBox" layoutX="484.0" layoutY="35.0" onAction="#comboEvent1" prefWidth="150.0" promptText="Select City" />
      <Button fx:id="modifySiteButton" layoutX="511.0" layoutY="505.0" mnemonicParsing="false" onAction="#viewSites" prefHeight="25.0" prefWidth="132.0" text="Add/Remonve Sites" />
   </children>
</AnchorPane>
