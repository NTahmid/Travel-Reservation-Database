<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="379.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.SitesFXMLController">
   <children>
      <Button fx:id="backButton" layoutX="37.0" layoutY="30.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <Text layoutX="108.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Sites Name:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="siteNameField" layoutX="331.0" layoutY="88.0" />
      <Text layoutX="108.0" layoutY="164.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Sites Id:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="siteIdField" layoutX="331.0" layoutY="146.0" />
      <ComboBox fx:id="countryBox" layoutX="331.0" layoutY="212.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <ComboBox fx:id="cityBox" layoutX="331.0" layoutY="266.0" prefWidth="150.0" promptText="Select City" />
      <Button fx:id="enterButton" layoutX="278.0" layoutY="340.0" mnemonicParsing="false" onAction="#enterData" text="Enter" />
      <Text fx:id="warningText" fill="#f20606" layoutX="236.0" layoutY="324.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please fill up all the entries">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
