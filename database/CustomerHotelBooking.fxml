<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="492.0" prefWidth="622.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.CustomerHotelBookingController">
   <children>
      <ComboBox fx:id="countryBox" layoutX="424.0" layoutY="33.0" onAction="#comboEvent" prefWidth="150.0" promptText="Select Country" />
      <TableView fx:id="table" layoutX="77.0" layoutY="173.0" prefHeight="238.0" prefWidth="501.0" />
      <Button fx:id="roomViewButton" layoutX="276.0" layoutY="434.0" mnemonicParsing="false" onAction="#selectHotel" text="View Rooms" />
      <TextField fx:id="minRatingField" layoutX="58.0" layoutY="46.0" prefHeight="25.0" prefWidth="162.0" promptText="Enter Minimum Rating(1-5)" />
      <TextField fx:id="maxRatingField" layoutX="58.0" layoutY="94.0" prefHeight="25.0" prefWidth="162.0" promptText="Enter Maximum Rating(1-5)" />
      <Button fx:id="searchButton" layoutX="284.0" layoutY="127.0" mnemonicParsing="false" onAction="#searchHotel" prefHeight="25.0" prefWidth="67.0" text="Search" />
      <ComboBox fx:id="cityBox" layoutX="424.0" layoutY="81.0" prefWidth="150.0" promptText="Select City" />
      <Text fx:id="userNameText" layoutX="77.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Button fx:id="backButton" layoutX="25.0" layoutY="434.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
   </children>
</AnchorPane>
