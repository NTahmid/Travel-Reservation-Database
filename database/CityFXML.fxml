<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="330.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.CityFXMLController">
   <children>
      <Text layoutX="119.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter City:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="citynameField" layoutX="290.0" layoutY="67.0" />
      <Text layoutX="119.0" layoutY="145.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter City id:">
         <font>
            <Font name="Century Schoolbook" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="cityIdField" layoutX="290.0" layoutY="127.0" />
      <ComboBox fx:id="countryBox" layoutX="290.0" layoutY="200.0" prefWidth="150.0" promptText="Select Country" />
      <Button fx:id="enterButton" layoutX="278.0" layoutY="291.0" mnemonicParsing="false" onAction="#enterData" text="Enter" />
      <Button fx:id="backButton" layoutX="27.0" layoutY="26.0" mnemonicParsing="false" onAction="#getBack" text="Back" />
      <Text fx:id="warningText" fill="#f80202" layoutX="237.0" layoutY="266.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please fill up all the entries">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
