<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="320.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectver2.AdminHomePageFXMLController">
   <children>
      <ComboBox fx:id="tableBox" layoutX="225.0" layoutY="93.0" prefWidth="150.0" promptText="Select Table" />
      <Button fx:id="insertButton" layoutX="173.0" layoutY="174.0" mnemonicParsing="false" onAction="#InsertAction" text="Insert" />
      <Button fx:id="selectButton" layoutX="355.0" layoutY="174.0" mnemonicParsing="false" onAction="#selectAction" text="Select" />
      <Text fx:id="warningText" fill="#dd0707" layoutX="124.0" layoutY="285.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Entries to the selected table are not allowed to be manipulated">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Text>
      <Text fx:id="warningText1" fill="#dd0303" layoutX="228.0" layoutY="238.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please select a table first">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="40.0" layoutY="34.0" mnemonicParsing="false" onAction="#getBack" text="Logout" />
   </children>
</AnchorPane>
